# Supabase Project Credentials (find in Dashboard → Settings → API)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Project name (used for backup folder naming)
PROJECT_NAME=my-app

